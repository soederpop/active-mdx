{
  "name": "@active-mdx/desktop",
  "version": "0.3.2",
  "author": "Jon Soeder <jon@chicago.com>",
  "description": "A desktop app for working with @active-mdx projects",
  "private": true,
  "scripts": {
    "dev": "electron-esbuild dev",
    "build": "electron-esbuild build",
    "package": "electron-builder"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/css-minimizer-webpack-plugin": "^3.0.2",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/webpack-dev-server": "^4.1.0",
    "autoprefixer": "^10.3.5",
    "babel-loader": "^8.2.2",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "electron": "^16.0.2",
    "electron-builder": "^22.11.7",
    "electron-esbuild": "3.x",
    "electron-util": "^0.17.2",
    "fork-ts-checker-webpack-plugin": "^6.3.3",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss": "^8.3.7",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^7.1.0",
    "react-refresh": "^0.10.0",
    "tailwindcss": "^3.0.7",
    "terser-webpack-plugin": "^5.1.4",
    "typescript": "^4.4.3",
    "url": "^0.11.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.53.0",
    "webpack-dev-server": "^4.2.1"
  },
  "build": {
    "appId": "io.comp.myapp",
    "productName": "desktop2",
    "extraMetadata": {
      "name": "desktop2",
      "main": "main.js"
    },
    "files": [
      {
        "from": ".",
        "filter": [
          "package.json"
        ]
      },
      {
        "from": "dist/main"
      },
      {
        "from": "dist/renderer"
      }
    ],
    "win": {
      "target": [
        "zip"
      ]
    },
    "mac": {
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ]
    },
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "dependencies": {
    "@active-mdx/core": "^0.3.2",
    "@codesandbox/sandpack-react": "^0.10.10",
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@monaco-editor/react": "^4.3.1",
    "@tailwindcss/forms": "^0.4.0",
    "copy-webpack-plugin": "^10.2.0",
    "electron-log": "^3.0.0",
    "electron-store": "^8.0.1",
    "esbuild": "0.x",
    "execa": "^6.0.0",
    "hash-object": "^0.1.7",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
